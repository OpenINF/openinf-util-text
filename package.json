  "bugs": {
    "url": "https://github.com/openinf/util-text/issues"
  },
  "homepage": "https://github.com/openinf/util-text#readme",
  "devDependencies": {
    "@types/cli-color": "^2.0.0",
    "@types/node": "^14.14.22",
    "jsdoc-to-markdown": "^6.0.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "cli-color": "^2.0.0",
    "has-unicode": "^2.0.1",
    "supports-ansi": "^1.0.6"
  }
}
