  "homepage": "https://github.com/openinf/util-text#readme",
  "devDependencies": {
    "@types/cli-color": "2.0.2",
    "@types/node": "18.7.15",
    "jsdoc-to-markdown": "7.1.1",
    "tape": "5.6.0",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "cli-color": "2.0.3",
    "has-unicode": "2.0.1",
    "supports-ansi": "1.0.6"
  }
