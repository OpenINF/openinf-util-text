{
  "name": "@openinf/util-text",
  "version": "1.0.0",
  "description": "Essential cross-platform terminal-friendly text utilities",
  "main": "./index.js",
  "types": "./index.d.ts",
  "scripts": {
    "compile": "npm run compile-src && npm run compile-doc",
    "compile-src": "tsc --project ./",
    "compile-doc": "jsdoc2md --template README.hbs --files index.js > ./README.md",
